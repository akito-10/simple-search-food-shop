{
  "name": "simple-search-food-shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --ignorePath .gitignore --output src/lib --watch",
    "build": "pathpida --enableStatic --ignorePath .gitignore --output src/lib && next build",
    "start": "next start",
    "serve": "run-s build start",
    "lint": "next lint",
    "prettier": "prettier --check .",
    "fix": "run-s -l -c fix:lint fix:prettier",
    "fix:lint": "next lint --fix",
    "fix:prettier": "prettier --write .",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@mantine/core": "^5.0.0",
    "@mantine/hooks": "^5.0.0",
    "@mantine/next": "^5.1.1",
    "@react-google-maps/api": "^2.12.1",
    "@reduxjs/toolkit": "^1.8.3",
    "autoprefixer": "^10.4.8",
    "axios": "^0.27.2",
    "next": "12.2.2",
    "postcss": "^8.4.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.2",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "swr": "^1.3.0",
    "tabler-icons-react": "^1.53.0",
    "tailwindcss": "^3.1.8"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "@types/axios": "^0.14.0",
    "@types/jest": "^28.1.6",
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-slick": "^0.23.10",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "babel-loader": "^8.2.5",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import-access": "^1.0.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.18.1",
    "prettier": "^2.7.1",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix .",
      "prettier --write ."
    ]
  }
}
